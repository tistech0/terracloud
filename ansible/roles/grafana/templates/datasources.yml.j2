# datasources.yml.j2
apiVersion: 1

datasources:
  - name: "Prometheus"
    type: prometheus
    access: proxy
    url: "http://localhost:{{ prometheus_port }}"
    isDefault: true
    jsonData:
      timeInterval: "15s"
      httpMethod: "GET"
      manageAlerts: true
    version: 1
    editable: false